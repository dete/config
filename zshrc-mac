# xcrun aliases

TOOLS=(svn otool)

for tool in $TOOLS; do
   if ! which $tool >&/dev/null && which xcrun >&/dev/null; then
      alias $tool="xcrun $tool"
   fi
done

alias ff='open -a Xee'
alias mp="mplayer"

if [ -d /usr/local/texlive/2012/bin/x86_64-darwin ]; then
   export PATH="${PATH}:/usr/local/texlive/2012/bin/x86_64-darwin"
fi
